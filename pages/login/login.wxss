.container{
  box-sizing: border-box;
}
.login-page {
  width: 100%;
  height: 100%;
}
.login-page .login-bg {
  width: 100%;
  height: 100%;
  background-image: url(data:image/png;base64,);
  background-repeat: repeat-y;
  background-size: 100%;
  background-color: #fff7d0;
}
.gaoshiqing-logo {
  position: absolute;
  width: 750rpx;
  height: 405rpx;
  left: 0;
  top: 0;
}
.login-bottom {
  position: absolute;
  width: 750rpx;
  height: 313rpx;
  left: 0;
  bottom: 0;
}
.login-box {
  position: absolute;
  width: 704rpx;
  height: 712rpx;
  left: 50%;
  margin-left: -352rpx;
  top: 218rpx;
}
.login-box image {
  position: absolute;
  width: 100%;
  height: 100%;
}

.login-tip {
  width: 100%;
  color: #130600;
  font-size: 28rpx;
  text-align: center;
  position: absolute;
  left: 0;
  top: 460rpx;
}
.login-btn {
  width: 260rpx;
  height: 96rpx;
  line-height: 96rpx;
  border: 4rpx solid #080103;
  border-radius: 48rpx;
  background: #ffec79;
  text-align: center;
  color: #130600;
  font-size: 32rpx;
  position: absolute;
  left: 50%;
  margin-left: -130rpx;
  bottom: 60rpx;
}
