.container{
  box-sizing: border-box;
}

.slide-image {
  width: 100%;
}

.home_search {
  height: 54rpx;
  line-height: 54rpx;
  background: #f4f4f4;
  font-size: 24rpx;
  border-radius: 27rpx;
  margin: 20rpx 30rpx;
  text-align: center;
}

.flex-wrp {
  display: flex;
  margin: 45rpx 30rpx 85rpx;
  flex-direction:row;
}

.flex-item {
  flex: 1;
  text-align: center;
}

.flex-item image {
  width: 155rpx;
}

.home-content-wrapper {
  padding: 0 12px;
}

.home-content-inner {
  display: flex;
  flex-wrap:wrap;
  flex-direction:row;
  margin-right: -28rpx;
}

.home-content-inner homeitem {
  width: 33.33%;
}

.search {
  padding: 15rpx 30rpx;
  border-bottom: 2rpx solid #e7e7e7;
}
.search input,
.search .search-input {
  width: 100%;
  height: 53rpx;
  line-height: 53rpx;
  font-size: 24rpx;
  background-color: #f4f4f4;
  border-radius: 26rpx;
  text-align: center;
}
.search .search-input {
  color: #999;
  position: relative;
}
.search .search-input::after {
  content: '';
  display: block;
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 180rpx;
  top: 12rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 30rpx 30rpx;
}

.no_more {
  font-size: 24rpx;
  color: #666;
  text-align: center
}

.page-container {
  overflow-x: hidden
}
